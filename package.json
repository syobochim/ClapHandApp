{
  "name": "claphand",
  "version": "0.0.14",
  "author": "syobochim <syobochim@gmail.com> (https://github.com/syobochim)",
  "description": "Clap Hand App",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/syobochim/clap-hands-application",
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "electron-notarize": "^1.0.0",
    "electron-packager": "^15.1.0"
  },
  "dependencies": {
    "aws-appsync": "^1.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.5",
    "es6-promise": "^4.1.1",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "isomorphic-fetch": "^2.2.1",
    "ws": "^3.3.1"
  },
  "publish": {
    "provider": "github",
    "owner": "syobochim",
    "repo": "clap-hands-application",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.syobochim.ClapHand",
    "afterSign": "scripts/notarize.js",
    "publish": {
      "provider": "github"
    },
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist"
    }
  }
}